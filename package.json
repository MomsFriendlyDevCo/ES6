{
  "name": "@momsfriendlydevco/es6",
  "version": "1.3.0",
  "description": "Various ES6 utilities",
  "type": "module",
  "scripts": {
    "lint": "eslint lib test",
    "test": "mocha"
  },
  "keywords": [
    "es6",
    "import",
    "module",
    "backport"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MomsFriendlyDevCo/ES6.git"
  },
  "bugs": {
    "url": "https://github.com/MomsFriendlyDevCo/ES6/issues"
  },
  "homepage": "https://github.com/MomsFriendlyDevCo/ES6#readme",
  "author": "Matt Carter <m@ttcarter.com>",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "exports": {
    ".": {
      "require": "./lib/cjs/index.cjs",
      "default": "./lib/esm/esm.js"
    },
    "./*": "./lib/esm/*.js"
  },
  "imports": {
    "#es6/*": "./lib/esm/*.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "mocha": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module"
    }
  },
  "dependencies": {
    "debug": "^4.3.4",
    "execa": "^7.1.1",
    "globby": "^13.1.4"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.38.0",
    "mocha": "^10.2.0"
  }
}
